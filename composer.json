{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.10",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "algolia/search-bundle": "^5.1",
        "babdev/pagerfanta-bundle": "^3.7",
        "cocur/slugify": "^4.1",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.1.1",
        "doctrine/orm": "^2.11",
        "easycorp/easyadmin-bundle": "^4.0",
        "friendsofsymfony/ckeditor-bundle": "^2.4",
        "friendsofsymfony/jsrouting-bundle": "^3.2",
        "knpuniversity/oauth2-client-bundle": "^2.19",
        "league/oauth2-github": "^3.1",
        "league/oauth2-google": "^4.0",
        "liip/imagine-bundle": "^2.7",
        "oneup/flysystem-bundle": "^4.3",
        "pagerfanta/doctrine-orm-adapter": "^3.6",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "pugx/autocompleter-bundle": "^1.8",
        "scheb/2fa-bundle": "^6.0",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/expression-language": "6.0.*",
        "symfony/filesystem": "5.4.*",
        "symfony/finder": "6.0.*",
        "symfony/flex": "^1.17|^2",
        "symfony/form": "6.0.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/http-client": "6.0.*",
        "symfony/intl": "6.0.*",
        "symfony/lock": "6.0.*",
        "symfony/loco-translation-provider": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/messenger": "6.0.*",
        "symfony/mime": "6.0.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "6.0.*",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/polyfill-php80": "^1.25.0",
        "symfony/process": "6.0.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/rate-limiter": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "5.4.*",
        "symfony/stimulus-bundle": "^2.27",
        "symfony/string": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/ux-autocomplete": "^2.7",
        "symfony/ux-chartjs": "^2.14",
        "symfony/ux-dropzone": "^2.10",
        "symfony/ux-live-component": "^2.7",
        "symfony/ux-turbo": "^2.32",
        "symfony/ux-twig-component": "^2.7",
        "symfony/validator": "6.0.*",
        "symfony/web-link": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.17",
        "symfony/yaml": "6.0.*",
        "symfonycasts/reset-password-bundle": "^1.11",
        "symfonycasts/verify-email-bundle": "^1.8",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/html-extra": "^3.4",
        "twig/inky-extra": "^3.22",
        "twig/intl-extra": "^3.5",
        "twig/string-extra": "^3.4",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^1.19"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.17",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.17.10",
        "symfony/browser-kit": "6.0.*",
        "symfony/css-selector": "6.0.*",
        "symfony/debug-bundle": "6.0.*",
        "symfony/maker-bundle": "^1.50",
        "symfony/stopwatch": "6.0.*",
        "symfony/var-dumper": "6.0.*",
        "symfony/web-profiler-bundle": "6.0.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "php -dmemory_limit=512M bin/console cache:clear": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
