

{% for foodGroupParent in foodGroupParents %}
    <div class="flex items-center justify-center w-8">
        {% if foodGroupParent.id in listFgp %}
            <div style="background-color: {{ foodGroupParent.color }}" class="flex text-white text-sm font-bold w-5 h-5 rounded-full justify-center items-center"></div>
        {% else %}
            <div style="background-color: {{ foodGroupParent.color }}" class="text-white font-bold w-5 h-5 rounded-full opacity-20"></div>
        {% endif %}
    </div>
{% endfor %}